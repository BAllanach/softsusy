# Example input in SLHA format, and suitable for input to
# SOFTSUSY (v1.8 or higher): benchmark point - see arXiv:1109.3859
Block MODSEL		     # Select model
    1    0		     # non universal
    4    1                   # RPV
Block SMINPUTS		     # Standard Model inputs
    1	1.279340000e+02	     # alpha^(-1) SM MSbar(MZ)
    2   1.166370000e-05	     # G_Fermi
    3   1.172000000e-01	     # alpha_s(MZ) SM MSbar
    4   9.118760000e+01	     # MZ(pole)
    5	4.250000000e+00	     # mb(mb) SM MSbar
    6   1.733000000e+02	     # mtop(pole)
    7	1.777000000e+00	     # mtau(pole)
Block MINPAR		     # Input parameters
    3   1.500000000e+00	     # tanb
Block SOFTSUSY               # Optional SOFTSUSY-specific parameters
    1   1.000000000e-03      # Numerical precision: suggested range 10^(-3...-6)
    2   0.000000000e+00	     # Quark mixing parameter: see manual
    5   1.000000000e+00      # Include 2-loop scalar mass squared/trilinear RGEs
#    8   1.000000000e+00	     # MSUSY-scale RPV boundary conditions
Block EXTPAR          # non-universal SUSY breaking parameters
      0   -1.000000000000000e+00	 # Set MX=MSUSY 
      1    2.000000000000000e+03         # M_1(MX)
      2    2.000000000000000e+03         # M_2(MX)
      3    2.000000000000000e+03         # M_3(MX)
     11    0.000000000000000e+00         # At(MX)
     12    0.000000000000000e+00         # Ab(MX)
     13    0.000000000000000e+00         # Atau(MX)
     23    1.000000000000000e+02         # mu(MX)
     26    1.000000000000000e+03         # mA(pole)
     31    1.000000000000000e+03         # meL(MX)
     32    2.000000000000000e+03         # mmuL(MX)
     33    8.800000000000000e+01         # mtauL(MX)
     34    1.000000000000000e+03         # meR(MX)
     35    1.000000000000000e+03         # mmuR(MX)
     36    8.000000000000000e+01         # mtauR(MX)
     41    1.000000000000000e+03         # mqL1(MX)
     42    1.000000000000000e+03         # mqL2(MX)
     43    1.000000000000000e+03         # mqL3(MX)
     44    1.000000000000000e+03         # muR(MX)
     45    1.000000000000000e+03         # mcR(MX)
     46    1.000000000000000e+03         # mtR(MX)
     47    1.000000000000000e+03         # mdR(MX)
     48    1.000000000000000e+03         # msR(MX)
     49    1.000000000000000e+03         # mbR(MX)
Block SOFTSUSY
    1   1.000000000e-03
    2   2.000000000e+00      # quark mixing: none (=0), up (=1) or down (=2)
    9   0.000000000e+00	     # Use normal hierarchy(=0.0) or inverted (=1.0)
Block VCKMIN		     # input parameters for CKM matrix
    1   2.272000000e-01      # lambda
    2   8.180000000e-01      # A
    3   2.210000000e-01      # rhobar
    4   3.400000000e-01      # etabar
Block RVLAMLQDIN             # MSUSY scale RPV trilinear coupling
 1 1 1  4.000000000e-01      # lam'_{111}
 2 1 1  1.000000000e-05      # lam'_{111}
Block RVTLLEIN               # MSUSY scale RPV trilinear coupling
 1 2 2  1.000000000e+03      # lam'_{111}
